@SET CELLDM 20.99386800 
@SET POT_RCUT 10.0 
@SET SAMPLE FCC_LJ 
@SET MD_EACH 100
@SET MD_RESTART 1000

 &GLOBAL
   BLACS_GRID  SQUARE
   PREFERRED_FFT_LIBRARY  FFTW
   PRINT_LEVEL HIGH
   PROGRAM_NAME CP2K
   PROJECT_NAME ${SAMPLE}
   RUN_TYPE MD
 &END GLOBAL

 &MOTION

   &MD
     ENSEMBLE  NVE
     STEPS        0
     TIMESTEP     0.5
     TEMPERATURE  0.0
   &END MD

&END MOTION

&FORCE_EVAL

 METHOD FIST
 STRESS_TENSOR ANALYTICAL

&MM
&FORCEFIELD
&SPLINE
RCUT_NB ${POT_RCUT}
UNIQUE_SPLINE T
#NPOINTS 1000
EPS_SPLINE 1.00000000E-009
&END SPLINE

IGNORE_MISSING_CRITICAL_PARAMS T
SHIFT_CUTOFF F 

&NONBONDED
&LENNARD-JONES
 atoms Ar Ar
 EPSILON [eV]  0.010323576
 SIGMA   3.405 
 RCUT   ${POT_RCUT}
&END LENNARD-JONES

&END NONBONDED


# ===================================================
#Â                     CHARGES 
# ===================================================
#&CHARGE
#atom Ar
#CHARGE 0
#&END CHARGE



&END FORCEFIELD

&POISSON
 &EWALD
 EWALD_TYPE NONE
 &END EWALD
&END POISSON

&NEIGHBOR_LISTS
VERLET_SKIN 1.0
&END NEIGHBOR_LISTS


&END MM


&SUBSYS

  &CELL

        A [angstrom] ${CELLDM}  0.00000000000  0.00000000000
        B [angstrom]  0.00000000000 ${CELLDM}  0.00000000000
        C [angstrom]  0.00000000000  0.00000000000 ${CELLDM}

  &END CELL

