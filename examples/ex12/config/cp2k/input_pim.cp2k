@SET POT_RCUT 5.0
@SET SAMPLE SiO2
@SET MD_EACH 1
@SET MD_RESTART 1000

 &GLOBAL
 !  BLACS_GRID  SQUARE
 !  PREFERRED_FFT_LIBRARY  FFTW
 !  PRINT_LEVEL HIGH 
   PRINT_LEVEL LOW 
   PROGRAM_NAME CP2K
   PROJECT_NAME ${SAMPLE}
   RUN_TYPE MD
 &END GLOBAL

 &MOTION
   &PRINT
     &FORCES
       &EACH 
         MD ${MD_EACH}
       &END EACH
     &END
     &TRAJECTORY
       &EACH 
         MD ${MD_EACH}
       &END EACH
     &END
     &CELL
       &EACH 
         MD ${MD_EACH}
       &END EACH
     &END
   &END

   &MD
     ENSEMBLE  NVT
     STEPS        1000
     TIMESTEP     0.5
     TEMPERATURE  2500.0
     &THERMOSTAT
      TYPE NOSE
      &NOSE
        LENGTH 3
	YOSHIDA 3
        MTS 2
        TIMECON 1000.0
      &END NOSE
#     TYPE CSVR
#     &CSVR
#     TIMECON 1000.0
#     &END CSVR
     &END THERMOSTAT

     &PRINT
       &ENERGY
         &EACH 
           MD ${MD_EACH}
         &END EACH
       &END ENERGY
       &PROGRAM_RUN_INFO
         &EACH 
           MD ${MD_EACH}
         &END EACH
       &END
     &END


   &END MD

&END MOTION

&FORCE_EVAL


  &PRINT
    &PROGRAM_RUN_INFO
      &EACH
      MD ${MD_EACH}
      &END
    &END
    #&FORCES
    #  &EACH
    #  MD ${MD_EACH}
    #  &END
    #&END
  &END

